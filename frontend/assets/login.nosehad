<!DOCTYPE html>
<html lang="en">
<head>
    <style>
@font-face {
  font-family: "Changa";
  src: url("/static/fonts/Changa-VariableFont_wght.ttf") format("truetype");
}
@font-face {
  font-family: "Major Mono Display";
  src: url("/static/fonts/MajorMonoDisplay-Regular.ttf") format("truetype");
}
@font-face {
  font-family: "Nunito";
  src: url("/static/fonts/Nunito-VariableFont_wght.ttf") format("truetype");
}
.msg-wrap,
.msg-steve-wrap {
  line-height: 20px;
  display: flex;
  margin-bottom: 10px;
  margin-right: 10px;
  margin-left: 10px;
  max-width: 352px;
}
.msg-wrap {
  opacity: 1;
}
.msg-personal {
  justify-self: right;
}
.msg-recieved {
  justify-self: left;
}
.msg-personal msg {
  background: #598fc6;
  border-radius: 5px 0px 5px 5px;
  margin-right: 10px;
}
.msg-recieved msg {
  background-color: #ca6d4c;
  border-radius: 0px 5px 5px 5px;
  margin-left: 10px;
}
.msg-steve-wrap {
  opacity: 0;
  animation-name: appear;
  animation-duration: 4s;
}
.inbox {
  position: fixed;
  top: 10px;
  right: 0px;
  z-index: 2;
}
.msg-steve-sender,
.msg-sender {
  height: 50px;
  width: 50px;
  min-width: 50px;
  border-radius: 25px;
  user-select: none;
  background: #a29d99;
  overflow: hidden;
}
.msg-sender img {
  height: 50px;
  width: auto;
}
.msg-sender name {
  font-size: 30px;
  font-family: "Major Mono Display";
  margin-top: 14px;
  display: block;
  margin-left: 8px;
  letter-spacing: -11px;
}
.msg-steve-content {
  line-height: 20px;
  font-size: 15px;
}
@keyframes pop-in {
  0% {
    margin-right: -295px;
  }
  25% {
    margin-right: 10px;
  }
  50% {
    transform: rotate(3deg);
  }
  75% {
    transform: rotate(-2deg);
  }
  100% {
    transform: rotate(0deg);
  }
}
@keyframes appear {
  0% {
    opacity: 1;
  }
  85% {
    margin-right: 10px;
  }
  95% {
    margin-right: -280px;
    opacity: 1;
  }
  96% {
    opacity: 0;
  }
}
msg {
  padding: 5px;
  animation-name: pop-in;
  animation-duration: 1s;
  overflow-x: auto;
}
msg * {
   max-width: 272px;
}
msg strong, msg a, msg hr {
   color: #fff !important;
}
msg img {
   max-height: 272px;
}
.msg-personal msg .grid-wrap {
  justify-content: right;
}
.msg-recieved msg .grid-wrap {
  justify-content: left;
}
msg p {
  margin-top: 2px;
  margin-bottom: 0px;
  font-size: 14px;
  font-weight: 400;
  font-family: "Nunito";
}
.msg-steve-title,
.msg-title {
  justify-self: flex-end;
  font-size: 13px;
  margin-right: 0px;
  user-select: none;
  background: #E4DFDA;
  padding: 0px 4px 0px 4px;
  border-radius: 5px;
}
.msg-bot-tag {
  background: #598fc6;
  line-height: 10px;
  font-size: 12px;
  padding: 5px;
  border-radius: 5px;
  margin-right: 10px;
  user-select: none;
}
@keyframes
  body-fade {
  0% {
    transform: translateX(-100%);
  }
  100% {
    transform: translateX(0%);
  }
}
body,
html {
  overflow-x: hidden;
  background-color: #a29d99;
  animation-name: body-fade;
  animation-duration: 0.5s;
  animation-timing-function: ease-out;
  background: #E4DFDA;
  background-size: cover;
  color: #0D1317;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
  width: 100%;
  font-family: "Changa", sans-serif, "Noto Color Emoji", "Twemoji",
    "Apple Color Emoji";
  font-optical-sizing: auto;
  font-weight: 300;
  font-style: normal;
  margin: 0;
  padding: 0;
  scroll-behavior: smooth;
}
::selection {
  color: #0D1317;
  background: #2d6954;
  height: 10px;
}
p {
  line-height: 18px;
  overflow-wrap: anywhere;
}
h1 {
  font-weight: 400;
  line-height: 32px;
}
footer {
  text-align: center;
  display: block;
}
:root {
  scroll-behavior: smooth;
  scrollbar-color: #0D1317 transparent;
  scrollbar-track-color: transparent;
  scrollbar-highlight-color: #2d6954;
  scrollbar-3dlight-color: #2d6954;
  scrollbar-arrow-color: #2d6954;
  scrollbar-base-color: #2d6954;
  scrollbar-darkshadow-color: #2d6954;
  scrollbar-face-color: #2d6954;
  scrollbar-shadow-color: #2d6954;
}
::-webkit-scrollbar {
  width: 10px;
}
::-webkit-scrollbar-track {
  background: transparent;
}
::-webkit-scrollbar-thumb {
  background: #2d6954;
  border-radius: 2px;
}
::-webkit-scrollbar-thumb:hover {
  background: #2d6954;
}
.content-wrap {
  display: block;
  align-self: center;
  margin: 40px;
}
logo {
  font-family: "Major Mono Display", monospace;
  font-weight: 400;
  font-style: normal;
  display: inline;
  user-select: none;
}
.flex-wrap {
  display: flex;
}
.grid-wrap {
  display: grid;
  grid-auto-flow: column;
  justify-content: space-between;
}
.sep-wrap {
  display: grid;
  grid-auto-flow: column;
}
.align-center {
  text-align: center;
  align-items: center;
}
a {
  text-decoration: none;
  cursor: pointer;
  color: #2d6954;
  user-select: none;
}
.grid {
  display: grid;
}
.margin-top {
  margin-top: 20px;
}
.margin-bottom {
  margin-bottom: 20px;
}
.sep-left {
  background-color: black;
  height: 2px;
  margin-left: 5px;
}
.sep {
  background-color: black;
  height: 2px;
}
.sep-right {
  background-color: black;
  height: 2px;
  margin-right: 5px;
}
.input-wrap {
  display: grid;
  text-align: left;
  grid-auto-flow: column;
}
.input-a-right {
  justify-self: flex-end;
  margin-right: 5px;
}
.input-a-left {
  justify-self: flex-start;
  margin-left: 5px;
}
.checkbox {
  display: block;
  position: relative;
  padding-left: 27px;
  cursor: pointer;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}
.checkbox input {
  position: absolute;
  opacity: 0;
  cursor: pointer;
  height: 0;
  width: 0;
}
.checkmark {
  position: absolute;
  top: 0;
  left: 0;
  height: 20px;
  width: 20px;
  border: 2px solid #847f7b;
  background: #E4DFDA;
  margin-top: 3px;
  border-radius: 3px;
  transition: all 0.3s;
}
.checkbox:hover input ~ .checkmark {
  background: #c2bdb8;
}
.checkbox input:checked ~ .checkmark {
  color: #E4DFDA;
  background: #c2bdb8;
}
.checkmark::after {
  content: "";
  position: absolute;
  display: none;
}
.checkbox input:checked ~ .checkmark:after {
  display: block;
}
.checkbox .checkmark:after {
  left: 8px;
  top: 5px;
  width: 3px;
  height: 7px;
  border: solid white;
  border-width: 0 3px 3px 0;
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  transform: rotate(45deg);
}
input[type]:not([type="checkbox"]) {
  padding: 10px;
  margin: 5px;
  color: #0D1317;
  background: #E4DFDA;
  border-radius: 5px;
  font-size: 15px;
  font-family: "Nunito", sans-serif;
  font-optical-sizing: auto;
  font-weight: 300;
  font-style: normal;
  transition: all 0.3s;
}
input[type]:not([type="checkbox"]):not(.invalid) {
  border: 2px solid #847f7b;
}
input.invalid {
  border: 2px solid #E63946;
}
input[type]:not([type="checkbox"])::after {
  content: "\0062";
}
input::-ms-reveal {
  display: none;
}
input::placeholder {
  color: #0D1317;
}
input[type]:not([type="checkbox"]):focus,
input[type]:not([type="checkbox"]):hover {
  outline: none;
  background: #c2bdb8;
}
input:focus::placeholder,
input:hover::placeholder {
  color: #0D1317;
}
input[type="file"]:hover {
  outline: none;
}
input[type="file"] {
  display: none;
}
.upload-label {
  display: block;
  padding: 10px;
  color: #0d1317;
  background: #e4dfda;
  border-radius: 5px;
  font-size: 15px;
  font-family: "Nunito", sans-serif;
  font-optical-sizing: auto;
  font-weight: 300;
  font-style: normal;
  transition: all 0.3s;
  border: 2px solid #847f7b;
  cursor: pointer;
}
.upload-wrap {
  margin: 5px;
}
.upload-label:hover,
.upload-label:focus {
  outline: none;
  background: #c2bdb8;
}
input[type="checkbox"]::after {
  content: "";
  position: absolute;
  display: none;
}
input:checked ~ input[type="checkbox"]::after {
  display: block;
}
input[type="checkbox"]::after {
  left: 9px;
  top: 5px;
  width: 5px;
  height: 10px;
  border: solid white;
  border-width: 0 3px 3px 0;
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  transform: rotate(45deg);
}
textarea::placeholder {
  color: #0D1317;
}
button {
  padding: 10px;
  margin: 5px;
  color: #0D1317;
  border: 2px solid #847f7b;
  background: #E4DFDA;
  border-radius: 5px;
  cursor: pointer;
  transition: all 0.3s;
  min-width: 120px;
  font-family: "Nunito", sans-serif;
  font-optical-sizing: auto;
  min-width: 130px;
  font-style: normal;
}
button svg {
  display: inline;
  margin-bottom: -2px;
  margin-right: 1px;
  width: 14px;
  height: 14px;
}
a svg {
  display: inline;
  margin-bottom: -2px;
  margin-right: 1px;
  width: 14px;
  height: 14px;
}
button:hover {
  outline: none;
  background: #c2bdb8;
}
.file-input-wrap {
  position: relative;
  overflow: hidden;
}
.file-input-wrap label {
  display: block;
  background-color: #a29d99;
  color: #F2EFE9;
  border-radius: 5px;
  cursor: pointer;
  padding: 10px;
  font-size: 14px;
  margin: 5px;
  transition: all 0.2s;
}
.file-input-wrap label:hover {
  outline: none;
  box-shadow: 0 0 5px #2d6954;
}
.hidden {
  display: none;
}
.hover-label {
  position: absolute;
  animation: label 1s ease-out 0s 1;
  padding: 10px;
  background: #E4DFDA;
  border: 2px solid #a29d99;
  border-radius: 5px;
  line-height: 12px;
  transform: translateX(calc(-50%));
  color: #2d6954;
  font-weight: 600;
}
.hover-label:after {
  content: "";
  position: absolute;
  bottom: -10px;
  left: calc(50% - 7.5px);
  border-left: 15px solid transparent;
  border-right: 0px solid transparent;
  border-top: 10px solid #a29d99;
}
@keyframes label {
  0% {
    opacity: 0;
  }
  85% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
.login-container {
  background-color: #a29d99;
  height: 80%;
  text-align: left;
  display: flex;
  flex-direction: column;
  justify-content: center;
  height: fit-content;
  max-width: 360px;
}
.setup-progress {
  position: fixed;
  top: 5px;
  left: 5px;
  border: 2px solid #a29d99;
  padding: 30px;
  border-radius: 5px;
  animation-name: setup-progress;
  animation-duration: 1s;
  animation-timing-function: ease-out;
}
@keyframes setup-progress {
 from {
  left: -220px;
 }
 to {
  left: 5px;
 }
}
.setup-progress h5 {
  margin-top: 0px;
  margin-bottom: 0px;
  font-size: 15px;
}
.login-cont-align {
  margin-left: 5px;
}
.logo-wrap {
  border: 2px solid #a29d99;
  display: grid;
}
.login-radix-logo {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 30px;
  justify-self: center;
}
@media only screen and (min-width: 500px) {
  .login-wrap {
    display: flex;
  }
  .login-radix-logo {
    writing-mode: tb-rl;
  }
  .logo-wrap {
    border-radius: 5px 0px 0px 5px;
  }
}
@media only screen and (max-width: 499px) {
  .login-wrap {
    display: flex;
    flex-direction: column;
  }
  .login-radix-logo {
    writing-mode: horizontal-tb;
  }
  .logo-wrap {
    border-radius: 5px 5px 0px 0px;
  }
}
@media only screen and (max-width: 390px) {
  body {
    align-items: flex-end;
    justify-content: flex-end;
  }
  .login-container {
    min-width: 100%;
    min-height: 400px;
  }
  .login-wrap {
    min-width: 100%;
  }
}
