<!DOCTYPE html>
<html lang="en">
<head>
    <title>Radix Chat - Login</title>
    <style>
@font-face {
  font-family: "Changa";
  src: url("/static/fonts/Changa-VariableFont_wght.ttf") format("truetype");
}
@font-face {
  font-family: "Major Mono Display";
  src: url("/static/fonts/MajorMonoDisplay-Regular.ttf") format("truetype");
}
@font-face {
  font-family: "Nunito";
  src: url("/static/fonts/Nunito-VariableFont_wght.ttf") format("truetype");
}
.msg-wrap,
.msg-steve-wrap {
  line-height: 20px;
  display: flex;
  margin-bottom: 10px;
  margin-right: 10px;
  margin-left: 10px;
  max-width: 352px;
}
.msg-wrap {
  opacity: 1;
}
.msg-personal {
  justify-self: right;
}
.msg-recieved {
  justify-self: left;
}
.msg-personal msg {
  background: #508CA4;
  border-radius: 5px 0px 5px 5px;
  margin-right: 10px;
}
.msg-recieved msg {
  background-color: #FFE74C;
  border-radius: 0px 5px 5px 5px;
  margin-left: 10px;
}
.msg-steve-wrap {
  opacity: 0;
  animation-name: appear;
  animation-duration: 4s;
}
.inbox {
  position: fixed;
  top: 10px;
  right: 0px;
  z-index: 2;
}
.msg-steve-sender,
.msg-sender {
  height: 50px;
  width: 50px;
  min-width: 50px;
  border-radius: 25px;
  user-select: none;
  background: #fff;
  overflow: hidden;
}
.msg-sender img {
  height: 50px;
  width: auto;
}
.msg-sender name {
  font-size: 30px;
  font-family: "Major Mono Display";
  margin-top: 14px;
  display: block;
  margin-left: 8px;
  letter-spacing: -11px;
}
.msg-steve-content {
  line-height: 20px;
  font-size: 15px;
}
@keyframes pop-in {
  0% {
    margin-right: -295px;
  }
  25% {
    margin-right: 10px;
  }
  50% {
    transform: rotate(3deg);
  }
  75% {
    transform: rotate(-2deg);
  }
  100% {
    transform: rotate(0deg);
  }
}
@keyframes appear {
  0% {
    opacity: 1;
  }
  85% {
    margin-right: 10px;
  }
  95% {
    margin-right: -280px;
    opacity: 1;
  }
  96% {
    opacity: 0;
  }
}
msg {
  padding: 5px;
  animation-name: pop-in;
  animation-duration: 1s;
}
msg .grid-wrap {
  justify-content: right;
}
msg p {
  margin: 0px;
  font-size: 14px;
  font-weight: 300;
  font-family: "Nunito";
}
.msg-steve-title,
.msg-title {
  justify-self: flex-end;
  font-size: 12px;
  margin-right: 5px;
  user-select: none;
}
.msg-bot-tag {
  background: #508CA4;
  line-height: 10px;
  font-size: 12px;
  padding: 5px;
  border-radius: 5px;
  margin-right: 10px;
  user-select: none;
}
  @keyframes
  body-fade {
  0% {
    transform: translateX(-100%);
  }
  100% {
    transform: translateX(0%);
  }
}
body,
html {
  overflow-x: hidden;
  background-color: #fff;
  animation-name: body-fade;
  animation-duration: 0.5s;
  animation-timing-function: ease-out;
  background: #E4DFDA;
  background-size: cover;
  color: #0D1317;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
  width: 100%;
  font-family: "Changa", sans-serif, "Noto Color Emoji", "Twemoji",
    "Apple Color Emoji";
  font-optical-sizing: auto;
  font-weight: 200;
  font-style: normal;
  margin: 0;
  padding: 0;
}
::selection {
  color: #0D1317;
  background: #E63946;
  height: 10px;
}
p {
  line-height: 18px;
}
h1 {
  font-weight: 400;
  line-height: 32px;
}
footer {
  text-align: center;
  display: block;
}
.content-wrap {
  display: block;
  align-self: center;
  margin: 40px;
}
logo {
  font-family: "Major Mono Display", monospace;
  font-weight: 400;
  font-style: normal;
  display: inline;
  user-select: none;
}
.flex-wrap {
  display: flex;
}
.grid-wrap {
  display: grid;
  grid-auto-flow: column;
}
.align-center {
  text-align: center;
  align-items: center;
}
a {
  text-decoration: none;
  cursor: pointer;
  color: #E63946;
  user-select: none;
}
.grid {
  display: grid;
}
.margin-top {
  margin-top: 20px;
}
.margin-bottom {
  margin-bottom: 20px;
}
.sep-left {
  background-color: black;
  height: 2px;
  margin-left: 5px;
}
.sep {
  background-color: black;
  height: 2px;
}
.sep-right {
  background-color: black;
  height: 2px;
  margin-right: 5px;
}
.input-wrap {
  display: grid;
  text-align: left;
  grid-auto-flow: column;
}
.input-a-right {
  justify-self: flex-end;
  margin-right: 5px;
}
.input-a-left {
  justify-self: flex-start;
  margin-left: 5px;
}
.checkbox {
  display: block;
  position: relative;
  padding-left: 27px;
  cursor: pointer;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}
.checkbox input {
  position: absolute;
  opacity: 0;
  cursor: pointer;
  height: 0;
  width: 0;
}
.checkmark {
  position: absolute;
  top: 0;
  left: 0;
  height: 20px;
  width: 20px;
  border: 2px solid #252627;
  background: #E4DFDA;
  margin-top: 3px;
  border-radius: 3px;
  transition: all 0.3s;
}
.checkbox:hover input ~ .checkmark {
  background: #252627;
}
.checkbox input:checked ~ .checkmark {
  color: #E4DFDA;
  background: #252627;
}
.checkmark::after {
  content: "";
  position: absolute;
  display: none;
}
.checkbox input:checked ~ .checkmark:after {
  display: block;
}
.checkbox .checkmark:after {
  left: 8px;
  top: 5px;
  width: 3px;
  height: 7px;
  border: solid white;
  border-width: 0 3px 3px 0;
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  transform: rotate(45deg);
}
input[type]:not([type="checkbox"]) {
  padding: 10px;
  margin: 5px;
  color: #0D1317;
  background: #E4DFDA;
  border-radius: 5px;
  font-size: 15px;
  font-family: "Nunito", sans-serif;
  font-optical-sizing: auto;
  font-weight: 300;
  font-style: normal;
  transition: all 0.3s;
}
input[type]:not([type="checkbox"]):not(.invalid) {
  border: 2px solid #252627;
}
input.invalid {
  border: 2px solid #E63946;
}
input[type]:not([type="checkbox"])::after {
  content: "\0062";
}
input::-ms-reveal {
  display: none;
}
input::placeholder {
  color: #0D1317;
}
input[type]:not([type="checkbox"]):focus,
input[type]:not([type="checkbox"]):hover {
  outline: none;
  color: #E4DFDA;
  border: 2px solid #252627;
  background: #252627;
}
input:focus::placeholder,
input:hover::placeholder {
  color: #E4DFDA;
}
input[type="file"]:hover {
  outline: none;
  box-shadow: 0 0 5px #E63946;
}
input[type="checkbox"]::after {
  content: "";
  position: absolute;
  display: none;
}
input:checked ~ input[type="checkbox"]::after {
  display: block;
}
input[type="checkbox"]::after {
  left: 9px;
  top: 5px;
  width: 5px;
  height: 10px;
  border: solid white;
  border-width: 0 3px 3px 0;
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  transform: rotate(45deg);
}
button {
  padding: 10px;
  margin: 5px;
  color: #0D1317;
  border: 2px solid #252627;
  background: #E4DFDA;
  border-radius: 5px;
  cursor: pointer;
  transition: all 0.3s;
  min-width: 120px;
  font-family: "Nunito", sans-serif;
  font-optical-sizing: auto;
  min-width: 130px;
  font-style: normal;
}
button svg {
  display: inline;
  margin-bottom: -2px;
  margin-right: 1px;
  width: 14px;
  height: 14px;
}
a svg {
  display: inline;
  margin-bottom: -2px;
  margin-right: 1px;
  width: 14px;
  height: 14px;
}
button:hover {
  outline: none;
  color: #E4DFDA;
  border: 2px solid #252627;
  background: #252627;
}
.file-input-wrap {
  position: relative;
  overflow: hidden;
}
input[type="file"] {
  position: absolute;
  font-size: 10px;
  right: 0;
  top: 0;
  opacity: 0;
  cursor: pointer;
  z-index: -1;
}
.file-input-wrap label {
  display: block;
  background-color: #fff;
  color: #F2EFE9;
  border-radius: 5px;
  cursor: pointer;
  padding: 10px;
  font-size: 14px;
  margin: 5px;
  transition: all 0.2s;
}
.file-input-wrap label:hover {
  outline: none;
  box-shadow: 0 0 5px #E63946;
}
.hidden {
  display: none;
}
.hover-label {
  position: absolute;
  animation: label 1s ease-out 0s 1;
  padding: 10px;
  background: #E4DFDA;
  border: 2px solid #fff;
  border-radius: 5px;
  line-height: 12px;
  transform: translateX(calc(-50%));
  color: #E63946;
  font-weight: 600;
}
.hover-label:after {
  content: "";
  position: absolute;
  bottom: -10px;
  left: calc(50% - 7.5px);
  border-left: 15px solid transparent;
  border-right: 0px solid transparent;
  border-top: 10px solid #fff;
}
@keyframes label {
  0% {
    opacity: 0;
  }
  85% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
strong {
  color: #E63946;
}
body {
  justify-content: space-between;
  flex-direction: column;
}
nav {
  text-align: center;
  margin-top: 20px;
  border: 2px solid #fff;
  max-width: 600px;
  border-radius: 5px;
  display: grid;
  grid-auto-flow: column;
  align-content: space-between;
  width: 100%;
}
nav a {
  padding: 10px;
  color: #0D1317;
}
nav a.active {
  background-color: #252627;
  color: #E63946;
  border-radius: 5px;
}
.chat-content-wrap {
  align-self: center;
  padding: 40px;
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: space-between;
}
.chat-header-wrap {
  max-width: 600px;
}
.chat-header {
  border: 2px solid #fff;
  padding: 10px;
  border-radius: 5px 5px 0px 0px;
}
.chat-header h1 {
  margin-top: 0px;
}
.chat-header p {
  margin-top: 0px;
  margin-bottom: 0px;
}
.tda-topic-title {
  margin: 0px 0px 0px 1px;
}
.chat {
  height: 100%;
  border-left: 2px solid #fff;
  border-right: 2px solid #fff;
}
.chat-info {
  text-align: center;
  font-style: italic;
}
.desc {
  padding-left: 5px;
  border-left: 2px solid #252627;
  margin-left: 3px;
}
.chat-content {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.chat-input-wrap {
  max-width: 600px;
}
.chat-input {
  text-align: center;
  display: grid;
  grid-auto-flow: column;
  justify-content: center;
  align-items: center;
  position: relative;
}
.msg-input-buttons {
  position: absolute;
  bottom: 0;
  right: 0;
  margin: 0px 20px 7px 0px;
  border-radius: 5px;
  display: none;
}
.msg-input-buttons svg {
  border: none !important;
  width: 20px;
  height: 20px;
  transition: all 0.3s ease-out;
  color: #252627;
  cursor: pointer;
}
.msg-input-buttons svg:hover {
  transform: rotate3d(1, 1, 1, 10deg);
}
.chat-input textarea:focus ~ .msg-input-buttons svg {
  color: #E4DFDA;
}
.chat-input textarea:hover ~ .msg-input-buttons svg {
  color: #E4DFDA;
}
.chat-input-inner-wrap {
  border: 2px solid #fff;
  border-radius: 0px 0px 5px 5px;
  padding: 10px;
}
.chat-input .svg-r {
  padding: 12px;
  border-top-right-radius: 5px;
  border-bottom-right-radius: 5px;
}
@keyframes svg-r-fade-out {
  0% {
    opacity: 1;
  }
  100% {
    opacity: 0;
  }
}
.chat-input .svg-c {
  padding: 12px;
  border-radius: 5px;
}
.chat-input .svg-l {
  padding: 12px;
  border-top-left-radius: 5px;
  border-bottom-left-radius: 5px;
}
@keyframes svg-l-fade-out {
  0% {
    opacity: 1;
  }
  100% {
    opacity: 0;
  }
}
.chat-input > svg {
  cursor: pointer;
  border: 2px solid #252627;
  transition: all 0.3s;
}
.chat-input > svg:hover {
  outline: none;
  color: #E4DFDA;
  background: #252627;
}
.chat-input textarea {
  outline: none;
  resize: none;
  padding: 10px;
  margin: 5px;
  color: #0D1317;
  background: #E4DFDA;
  font-size: 15px;
  font-family: "Nunito", sans-serif;
  font-optical-sizing: auto;
  font-weight: 300;
  font-style: normal;
  transition: all 0.3s;
  min-width: 180px;
  text-align: center;
  border-top: 2px solid #252627;
  border-bottom: 2px solid #252627;
  border-left: none;
  border-right: none;
  border-radius: 0px;
  margin-left: 0px;
  margin-right: 0px;
  -ms-overflow-style: none;
  scrollbar-width: none;
}
.chat-input textarea::-webkit-scrollbar {
  display: none;
}
.chat-input textarea:focus,
.chat-input textarea:hover {
  color: #E4DFDA;
  background: #252627;
  outline: none;
}
.chat-input textarea::placeholder {
  color: #0D1317;
}
.chat-input textarea:focus::placeholder,
.chat-input textarea:hover::placeholder {
  color: #E4DFDA;
}
.display-inner {
  border: 2px solid #fff;
  padding: 10px;
  border-radius: 5px;
}
.clock {
  width: 200px;
  height: 200px;
}
.threads-display {
  width: 25%;
}
.threads-display ul {
  list-style-type: disclosure-closed;
  margin-block-start: 5px;
  margin-block-end: 0px;
  padding-inline-start: 22px;
  user-select: none;
}
.threads-display svg {
  cursor: pointer;
  transition: all 0.3s;
}
.threads-display li span {
  font-weight: 600;
  cursor: pointer;
  position: relative;
  line-break: anywhere;
}
.threads-display li span:hover:before {
  content: "";
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 2px;
  background-color: #E63946;
  animation: border-grow 0.7s ease-out 0s 1;
}
@keyframes border-grow {
  0% {
    width: 0;
  }
  100% {
    width: 100%;
  }
}
.threads-display svg:hover {
  transform: rotate3d(1, 1, 1, 10deg);
}
.display-inner-top {
  align-self: flex-start;
}
.display-inner-bottom {
  align-self: flex-start;
}
.statements-display {
  width: 25%;
  display: grid;
  align-content: space-between;
}
.statements-display ul {
  list-style-type: none;
  margin-block-start: 5px;
  margin-block-end: 0px;
  padding-inline-start: 2px;
}
q {
  margin-top: 15px;
  border-left: 2px solid #0D1317;
  padding-left: 10px;
  line-height: 25px;
  display: block;
}
q:before {
  display: none;
}
q:after {
  display: none;
}
q-creator {
  color: #508CA4;
  display: block;
  padding-left: 13px;
  line-height: 12px;
}
q-creator:before {
  content: "~ ";
}
.account-image {
  width: 200px;
  height: 200px;
  border-radius: 100%;
  overflow: hidden;
}
.account-image img {
  height: 200px;
}
.uname-inf {
  white-space: nowrap;
  overflow: hidden;
  width: calc(100% + 15px);
}
.uname-inf h1,
.uname-inf p {
  margin: 0px;
  overflow: hidden;
  padding: 0px;
  width: calc(100% + 13px);
}
.msg-storage {
  display: grid;
}
